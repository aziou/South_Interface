<Window x:Class="TheNewInterface.DownLoadPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DownLoadPage" Height="600" Width="600">
    <Grid Name="Grid_Total">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="10*"/>
            
        </Grid.RowDefinitions>
        <WrapPanel  VerticalAlignment="Center">
            <Label Content="表类型：" Width="80" Height="25" VerticalContentAlignment="Center" />
            <ComboBox Name="cmb_MeterType" Width="80" Height="25" VerticalContentAlignment="Center" />
            <Label Content="表位数：" Width="80" Height="25" VerticalContentAlignment="Center"/>
            <TextBox Name="txt_MeterNum" Width="80" Height="25" VerticalContentAlignment="Center"/>
            <Button Name="btn_click" Width="80" Height="25" Content="生成输入框" Click="btn_click_Click"/>
            <Button Name="btn_downLoad" Width="80" Height="25" Content="下载数据" Margin="5,0,0,0" Click="btn_downLoad_Click"/>
        </WrapPanel>
        <Grid Name="GenMeter" Grid.Row="1">
            
        </Grid>
    </Grid>
</Window>
