<Window x:Class="TheNewInterface.UI.InputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="连续生成铅封" Height="140" Width="300"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel>
            <Label Content="请输入起始铅封号:"/>
            <TextBox Name="txt_input" Width="250" Height="25" />
        </StackPanel>
        <Button VerticalAlignment="Bottom" HorizontalAlignment="Right" Name="btn_make" Height="25" Width="80" Content="生成编号" Margin="0,0,10,10" Click="btn_make_Click" />

    </Grid>
</Window>
